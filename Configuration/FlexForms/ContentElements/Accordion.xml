<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <ROOT>
        <type>array</type>
        <el>
            <accordions>
                <section>1</section>
                <type>array</type>
                <el>
                    <accordion>
                        <title>Accordion</title>
                        <type>array</type>
                        <el>
                            <header>
                                <TCEforms>
                                    <label>Überschrift</label>
                                    <config>
                                        <type>input</type>
                                    </config>
                                </TCEforms>
                            </header>
                            <bodytext>
                                <TCEforms>
                                    <label>Text</label>
                                    <config>
                                        <type>text</type>
                                        <cols>30</cols>
                                        <rows>5</rows>
                                        <enableRichtext>1</enableRichtext>
                                    </config>
                                    <defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
                                </TCEforms>
                            </bodytext>
                            <active>
                                <TCEforms>
                                    <label>Geöffnet</label>
                                    <config>
                                        <type>check</type>
                                    </config>
                                </TCEforms>
                            </active>
                        </el>
                    </accordion>
                </el>
            </accordions>
        </el>
    </ROOT>
</T3DataStructure>